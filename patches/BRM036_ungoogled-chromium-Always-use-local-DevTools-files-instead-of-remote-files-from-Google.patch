@@ -4,7 +4,7 @@
     { "name": "inspector_main", "type": "autostart" },
     { "name": "mobile_throttling", "type": "autostart" },
 
-    { "name": "accessibility", "type": "remote" },
+    { "name": "accessibility" },
     { "name": "animation" },
     { "name": "audits2" },
     { "name": "browser_console" },
@@ -12,14 +12,14 @@
     { "name": "cookie_table" },
     { "name": "devices" },
     { "name": "elements" },
-    { "name": "emulated_devices" , "type": "remote" },
