                                                &polling_delay_ms);
-
+  polling_delay_ms += (OmniboxFieldTrial::kDefaultMinimumTimeBetweenSuggestQueriesMs
+                       * base::RandDouble());
   base::TimeDelta delay(base::Milliseconds(polling_delay_ms));
   if (from_last_keystroke)
     return delay;
